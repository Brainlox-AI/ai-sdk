{
    "name": "@debales-ai/ai-sdk",
    "private": true,
    "scripts": {
        "build": "turbo build",
        "dev": "turbo dev",
        "lint": "turbo lint",
        "clean": "turbo clean && rm -rf node_modules",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "changeset": "changeset",
        "version-packages": "changeset version",
        "release": "turbo build --filter=docs^... && changeset publish"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.8",
        "@debales-ai/eslint-config": "workspace:*",
        "eslint": "^8.57.1",
        "prettier": "^3.3.3",
        "turbo": "latest"
    },
    "packageManager": "pnpm@9.1.0",
    "engines": {
        "node": ">=18"
    }
}
