{
    "name": "@debales-ai/ai-sdk",
    "version": "1.0.0",
    "type": "module",
    "files": [
        "dist/**"
    ],
    "sideEffects": [
        "**/*.css"
    ],
    "main": "./dist/ai-sdk.umd.cjs",
    "module": "./dist/ai-sdk.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/ai-sdk.js",
